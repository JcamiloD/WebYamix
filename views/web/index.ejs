/* Estilo para cuando se expande la tarjeta */
.card-body.expanded {
    height: auto; /* Permite que la tarjeta se expanda */
}

/* Ocultar el texto largo hasta que se haga clic en "ver más" */
.card-text + .ver-mas-btn {
    cursor: pointer;
    color: blue;
}


